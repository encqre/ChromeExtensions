{
    "manifest_version": 2,
    "name": "SpoilerProtector",
    "version": "0.1",
    "web_accessible_resources": [
        "replacer.jpg"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png"
    }
}